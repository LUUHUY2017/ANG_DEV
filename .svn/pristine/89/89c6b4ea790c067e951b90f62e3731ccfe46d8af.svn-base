.add_help {
  padding: 20px;
  text-transform: uppercase;
  font-family: Open-Bold;
  color: #007ad9 !important;
  cursor: pointer;
}

.add_help:hover {
  text-decoration: underline !important;
}

.linktag {
  cursor: pointer;
  color: #20a8d8 !important;
}

.linktag:hover {
  text-decoration: underline !important;
}

.spinner {
  color: #fff;
  position: absolute;
  top: 20px;
  left: 110px;
  font-size: 20px;
  animation: xoaytron 1s infinite;
}
.infinity {
  animation: xoaytron 1s infinite;
}
.noidungbook img {
  max-width: 100%;
}
@keyframes buttonhover {
  0% {
    transform: rotate(-20deg) !important;
  }
  50% {
    transform: rotate(20deg) !important;
  }
  to {
    transform: rotate(0) !important;
  }
}
@keyframes xoaytron {
  to {
    transform: rotate(360deg);
  }
}

@media only screen and (max-width: 768px) {
  .d-md-block {
    display: block !important;
  }
  .btn-group > .btn.active {
    z-index: 0;
  }
  .auto_scroll {
    max-height: 250px;
  }
}

@media only screen and (max-width: 1200px) {
  .side {
    height: 100%;
    position: fixed;
    transition: 0.3s;
    top: 0px;
    left: -300px;
    z-index: 3;
    background: #fff !important;
    width: 300px;
    overflow: auto;
    max-width: unset;
  }
  .left {
    margin-left: 300px !important;
  }
  .btnshow {
    background: #fff;
    display: block;
  }
  .app-body {
    margin-top: 40px;
    padding-top: 10px;
  }
  .grade-blur {
    position: fixed;
    top: 0;
    height: 100%;
    width: 100%;
    left: 0px;
    background: #00000073;
    z-index: 2;
  }
}

@media only screen and (max-width: 414px) {
  .d-md-block {
    margin-left: 15px;
  }
  .d-md-block label {
    font-size: 3vw;
  }
}

@media only screen and (max-width: 395px) {
  .d-md-block {
    margin-left: 15px;
  }
}

@media only screen and (max-width: 991px) {
  .sidebar {
    margin-left: -200px !important;
    position: absolute;
    top: -10px;
    left: 20px;
    width: 250px;
  }
  /*27/10*/
  .show_right_content {
    transition: 0.3s;
    width: 74% !important;
    transform: translateX(200px) !important;
  }
}
