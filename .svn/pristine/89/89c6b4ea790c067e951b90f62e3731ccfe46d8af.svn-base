body {
  font-family: Open-light !important;
}

@font-face {
  font-family: "Gothic";
  src: url("../../../fonts/yugothil.ttf");
}

@font-face {
  font-family: "Open-semi";
  src: url("../../../fonts/OpenSans-SemiBold.ttf");
}

@font-face {
  font-family: "Open-bold";
  src: url("../../../fonts/OpenSans-Bold.ttf");
}

@font-face {
  font-family: "Open-regular";
  src: url("../../../fonts/OpenSans-Regular.ttf");
}

@font-face {
  font-family: "Open-light";
  src: url("../../../fonts/Roboto-Light.ttf");
}
@import "~@ng-select/ng-select/themes/default.theme.css";
.link_icon:hover {
  text-decoration: underline;
}

.menu_ul {
  padding-left: 0px;
  list-style: none;
}

.menu_ul li {
  display: block;
  position: relative;
  border-radius: 0px 25px 25px 0px;
}

.menu_ul li:active {
  outline: unset;
}

.menu_ul .label1,
.menu_ul .label2 {
  padding: 5px 10px 0px 10px;
  color: rgba(0, 0, 0, 0.54);
  display: block;
  content: "";
  user-select: none;
  text-transform: uppercase;
  font-weight: bold;
  font-size: 11px;
  border-top: 0.6px solid rgba(0, 0, 0, 0.12);
}

.menu_ul > li > a {
  padding: 15px 0px 15px 10px;
}

// tesst
.menu_ul li > a {
  display: block;
  color: #747475 !important;
  cursor: pointer;
  position: relative;
  overflow: hidden;
  font-size: 13px;
  font-weight: bold;
  line-height: 10px;
  border-radius: inherit;
  user-select: none;
  transition: 0.3s ease-in;
}

.right_content {
  background: #f8f9fa;
}

.show_sub_menu {
  background: #dadce0;
}

.show_sub_menu > .has_children::after {
  transform: rotate(-90deg);
}

.show_sub_menu > ul.sub_menu_ul {
  max-height: 1300px;
  transition: max-height 0.1s ease-in;
}

.menu_ul li > a:hover {
  text-decoration: none;
  background: #f3b21c;
}

.add-padding {
  padding-left: 90px !important;
}

.has_children::after {
  position: absolute;
  top: 50%;
  right: 1rem;
  display: block;
  width: 8px;
  height: 8px;
  padding: 0;
  margin-top: -4px;
  content: "";
  background-image: url(data:image/svg+xml;charset=utf8;base64,);
  background-repeat: no-repeat;
  background-position: center;
  transition: 0.2s;
}

.sub_menu_ul {
  padding-left: 0px;
  margin-top: 0px;
  list-style: none;
  overflow: hidden;
  transition: 0.4s;
  max-height: 0px;
  transition: max-height 0.15s ease-out;
}

.sub_menu_ul > li > a {
  display: block;
  position: relative;
  padding: 15px 0px 15px 60px;
}

.ripple {
  border-radius: 50%;
  background-color: #ffffff61;
  position: absolute;
  transform: scale(0.6);
  animation: ripple 0.6s linear forwards;
}

@keyframes ripple {
  to {
    transform: scale(2.5);
    opacity: 0;
  }
}

.selected {
  // background: #dadce0;
  background: #f3b21c;
}

.navbar {
  display: -ms-flexbox;
  display: flex;
  -ms-flex-direction: column;
  flex-direction: column;
  color: #fff;
  background: linear-gradient(to right, rgb(26, 22, 50), rgb(8, 22, 42));
  height: 100%;
}

.navbardropdown {
  background: rgba(0, 0, 0, 0.2);
}

.column1 {
  width: 15%;
}

.column2 {
  width: 85%;
  margin-right: 10px;
}

.sidebar .nav-dropdown.open {
  background: rgba(0, 0, 0, 0.3);
}

.sidebar .nav-link.active {
  background: #575d61;
}

// Notifications
.flex_item {
  display: flex;
  align-items: center;
  justify-content: center;
}

.motthongbao {
  cursor: pointer;
  position: relative;
  padding: 8px 0px;
}

.motthongbao > span {
  position: absolute;
  top: 0px;
  color: crimson;
  right: 5px;
  opacity: 0;
  visibility: hidden;
  transition: 0.5s;
}

.motthongbao:hover {
  background: #ddd;
}

.motthongbao:hover span {
  opacity: 1;
  visibility: visible;
}

.auto_scroll {
  padding-top: 0px;
  padding-bottom: 0px;
  overflow-y: auto;
  max-height: 400px;
}

.unseen {
  background: #eee;
}

.seen {
  background: #fff;
}

.noidungnotfication > b {
  text-transform: capitalize;
}

