/*Nghĩa 26/10*/

.btnshow {
  position: absolute;
  top: 0;
  left: 0;
  display: none;
}

.group-line {
  width: fit-content;
}

.group-line:hover {
  cursor: pointer;
}

.line {
  width: 25px;
  border: 1px solid #8a8a8a;
  margin: 5px 0px;
}
/* ------------ Ng-Select Card----------------*/

.bootstrap {
  box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.2), 0 1px 1px 0 rgba(0, 0, 0, 0.14), 0 2px 1px -1px rgba(0, 0, 0, 0.12) !important;
  min-width: 100px;
}

select-dropdown > div.below {
  z-index: 5 !important;
}
/* ------------ Custom Card----------------*/

.card-parent {
  border: unset !important;
  border-radius: unset;
  box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.2), 0 1px 1px 0 rgba(0, 0, 0, 0.14), 0 2px 1px -1px rgba(0, 0, 0, 0.12) !important;
}

.card-header {
  border-bottom: 1px solid #ececec;
  background: #fff;
}

.card-child {
  border: 1px solid #f0f3f5 !important;
}

.card-body-bottom {
  padding: 20px 20px 8px 20px !important;
}

.table th,
.table td {
  border-top: 1px solid #ececec !important;
}
.table td {
  color: #808080;
}
/*---------------Thay đổi ngày giờ------------*/

.d-md-block {
  margin-bottom: 5px;
}

.d-md-block .btn-custom:not(:disabled):not(.disabled).active {
  color: #222;
  background-image: -webkit-linear-gradient(top, #f0f0f0, #dfdfdf);
  background-color: #c8ced3;
  border-color: #c8ced3;
  border-color: #ccc;
  font-weight: bold;
  box-shadow: inset 0px 1px 5px rgba(0, 0, 0, 0.3);
}

.d-md-block .btn-custom {
  color: #222;
  font-weight: bold;
  background-color: transparent;
  border-color: #c8ced3;
}

.table td {
  padding: 0.55rem 0.75rem;
}

