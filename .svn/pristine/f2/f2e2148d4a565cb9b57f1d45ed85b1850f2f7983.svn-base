.ml {
	margin-right: 0.5rem !important;
}

/*----------  Kết thúc áp dụng, chia sẻ  ----------*/

.fa-caret-down {
	padding-left: 3px;
}

.fba-body-body {
	width: 100%;
}

/* button ngày giờ */

.d-md-block {
	margin-bottom: 5px;
}

.d-md-block .btn-custom:not(:disabled):not(.disabled).active {
	color: #222;
	background-image: -webkit-linear-gradient(top, #f0f0f0, #dfdfdf);
	background-color: #c8ced3;
	border-color: #c8ced3;
	border-color: #ccc;
	font-weight: bold;
	box-shadow: inset 0px 1px 5px rgba(0, 0, 0, .3);
}

.d-md-block .btn-custom {
	color: #222;
	font-weight: bold;
	background-color: transparent;
	border-color: #c8ced3;
}

/*----------  Phần table  ----------*/

.table-reason  {
	width: 100%;
}
.table-reason table th, .table-reason table td {
	text-align: center;
}

.phan-tram-danh-gia {
	font-size: 14px !important;
	color: #898989 !important;
	font-weight: 400;
}

.table-reason table span#text {
	font-size: 11px;
	padding-left: 3px;
	color: #898989;
	font-weight: 400;
}

.table-reason table tr td {
	vertical-align: middle;
}

/* 22/11/2018 */

.table-reason table span#percent {
	font-size: 11px;
	display: block;
	font-weight: 400;
}

.table-reason table div {
	color: #333;
	font-size: 20px;
	font-weight: 400;
}

.table-reason thead th {
	padding: 5px 0;
}

.table-reason table th, .table-reason table td {
	text-align: center;
}

.table-reason table thead th {
	border-bottom: 1px solid #e4eaef !important;
	border-top: unset !important;
}

.table-reason table#table-cold-left tr td:nth-child(1) {
	text-align: left;
	padding-left: 3%;
}

.table-reason table span {
	font-size: 11px;
	padding-left: 2px;
	color: #898989;
}

.table-reason table th img {
	width: 30px;
	height: 30px;
}

.tong-danh-gia {
	font-size: 18px !important;
	font-weight: bold !important;
}

