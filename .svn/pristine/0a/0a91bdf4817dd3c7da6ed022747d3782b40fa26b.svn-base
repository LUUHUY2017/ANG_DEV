<block-ui></block-ui>
<div class="card card-parent">
    <div class="card-header">
        <div class="row mr-0 ml-0" id="time-comparison-header">
            <div class="col-sm-12 col-md-4 p-0">
                <app-menutree (submit_menu)="get_emit_menu($event)"></app-menutree>
            </div>
            <div class="col-sm-12 col-md-8 p-0">
                <div class="time-period-comparison">
                    <app-timeperiod class="time_input1 time_no_input1" #TimeInput (submit_date)="get_date_from_emit($event,1)">
                    </app-timeperiod>
                    <span style="padding: 0px 10px;font-size:11px;">So với</span>
                    <app-timeperiod class="time_input2 time_no_input2" #TimeInput_2 (submit_date)="get_date_from_emit($event,2)">
                    </app-timeperiod>
                </div>
            </div>
        </div>
    </div> <!-- end-card-header -->
    <div class="card-body card-body-bottom">
        <div class="row">
            <div class="container-fluid">
                <script src="ace-builds/src-min/ace.js"></script>
                <div class="animated fadeIn">
                    <div class="card card-child">
                        <div class="card-header">
                            <div class="row">
                                <ul id="headmapmenu_2">
                                    <li>
                                        <ng-select class="bootstrap selecttimeview select-visits" [options]="indexOption" [allowClear]="false" [(ngModel)]="indexOptionSelected"></ng-select>
                                    </li>
                                    <!-- li-visit -->
                                    <li>
                                        <div class="btn-group btn-group-toggle ml" data-toggle="buttons">
                                            <ng-select class="bootstrap selecttimeview starttime" [options]="startTimeOption" [allowClear]="false" [(ngModel)]="start_time">
                                            </ng-select>
                                            <span style="padding: 3px 0;">-</span>
                                            <ng-select class="bootstrap selecttimeview endtime" [options]="endTimeOption" [allowClear]="false" [(ngModel)]="end_time">
                                            </ng-select>
                                        </div>
                                        <button type="button" class="btn btn-primary btnapdung" [disabled]="btnApplyValid" (click)="get_data()">
                                            Áp dụng
                                        </button>
                                    </li>
                                    <!-- li-ap dung -->
                                    <li>
                                        <div>
                                            <!-- <div class="btn-group float-right" dropdown>
                                                <button dropdownToggle type="button"
                                                    class="btn btn-primary dropdown-toggle float-right btnchiase">
                                                    Chia Sẻ
                                                </button>
                                                <ul *dropdownMenu class="dropdown-menu float-right " role="menu">
                                                    <li role="menuitem"><a class="dropdown-item btn-brand btn-facebook"
                                                            href="https:www.facebook.com"><i
                                                                class="fa fa-facebook"></i>Facebook</a></li>
                                                    <li role="menuitem"><a
                                                            class="dropdown-item btn-brand btn-google-plus"
                                                            href="https:www.plus.google.com"><i
                                                                class="fa fa-google-plus"></i>G+</a></li>
                                                    <li role="menuitem"><a class="dropdown-item  " href="#">Email</a></li>
                                                </ul>
                                            </div> -->
                                            <div class="btn-group float-right" dropdown>
                                                <button dropdownToggle type="button" class="btn btn-primary dropdown-toggle float-right btnxuat"> Xuất
                                                </button>
                                                <ul *dropdownMenu class="dropdown-menu float-right" role="menu">
                                                    <li role="menuitem"><a style="cursor:pointer" class="dropdown-item float-right" (click)="exportExcel()">Xuất ra Excel</a></li>
                                                    <!-- <li role="menuitem"><a class="dropdown-item  float-right"  href="#">Xuất ra PDF</a> </li>
                                                    <li role="menuitem"><a class="dropdown-item  float-right" href="#">Other</a></li> -->
                                                </ul>
                                            </div>
                                        </div>
                                    </li>
                                    <!-- li- xuất -->
                                </ul>
                            </div>
                            <!-- end-row -->
                        </div> <!-- card-header -->
                        <div class="card-body card-body-bottom">
                            <div class="row">
                                <div class="col-sm-5">
                                </div>
                                <div class="col-sm-7 d-none d-md-block">
                                    <div class="btn-group btn-group-toggle float-right mr-3" data-toggle="buttons">
                                        <label class="btn btn-custom btn-outline-secondary" [(ngModel)]="viewDataBy" btnRadio="Hour" (click)="change_view()">Giờ</label>
                                        <label class="btn btn-custom btn-outline-secondary" [(ngModel)]="viewDataBy" btnRadio="Day" (click)="change_view()">Ngày</label>
                                        <label class="btn btn-custom btn-outline-secondary" [(ngModel)]="viewDataBy" btnRadio="Week" (click)="change_view()">Tuần</label>
                                        <label class="btn btn-custom btn-outline-secondary" [(ngModel)]="viewDataBy" btnRadio="Month" (click)="change_view()">Tháng</label>
                                        <label class="btn btn-custom btn-outline-secondary" [(ngModel)]="viewDataBy" btnRadio="Year" (click)="change_view()">Năm</label>
                                    </div>
                                </div>
                            </div>
                            <div class="row foot-title-show">
                                <!-- <span class="title-show1 ">
                                    {{site_name}}</span> &nbsp; &nbsp; {{ title_index }} &nbsp; &nbsp; {{ starttime }}x &nbsp;&minus;&nbsp; {{ endtime }} &nbsp; &nbsp; {{ title_time_period }}: {{ time_period }}&nbsp; <i style="border-bottom: 1px dotted #7b7b7b;"> so với </i> &nbsp;{{time_period_compare}}
                                </span> -->
                            </div>
                            <div class="row">
                                <div class="col-sm-12 col-md-12 ">
                                    <div #container1></div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12 col-md-12 ">
                                    <div style="border-top:1px dotted #a7a4a4" class="row">
                                        <div class="table-responsive">
                                            <table style="width: 100%;min-width: 900px" id="table" class="table table-striped table-comparation">
                                                <thead>
                                                    <tr>
                                                        <th style="width:250px;padding-left: 2.5rem;">Thời Gian</th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Visits' "> Visits </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Visits' "> Visits Compared </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Traffic Flow'"> Traffic Flow </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Traffic Flow'"> Traffic Flow Compared </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='PasserBy'"> PasserBy </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='PasserBy'"> PasserBy Compared </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Shopper Visits'"> Shopper Visits </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Shopper Visits'"> Shopper Visits Compared </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Kids Visits'"> Kids Visits </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Kids Visits'"> Kids Visits Compared</th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Turn in rate'"> Turn in rate (%)</th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Turn in rate'"> Turn in rate Compared (%)</th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Avg Time' "> Avg Time (min) </th>
                                                        <th style="width:250px" [hidden]="indexOptionSelected !=='Avg Time' "> Avg Time Compared (min)</th>
                                                        <th style="width:250px;font-size:22px;line-height: 20px;">
                                                            <span class="fa-icon-green "><i class="fa fa-caret-up"></i></span>
                                                            <span class="fa-icon-red pdl-5"><i class="fa fa-caret-down"></i></span>
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td></td>
                                                        <!-- tổng số lượng visit -->
                                                        <td [hidden]="indexOptionSelected !=='Visits' ">
                                                            <b>{{total_num_to_enter  |  number:'1.0-2'}}</b><br> <span class="spancell">Tổng Visits </span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Visits' ">
                                                            <b>{{total_num_to_enter_compare > 0 ? ( total_num_to_enter_compare  |  number:'1.0-2') : 0}}</b><br>
                                                            <span class="spancell">Tổng Visits được so sánh</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Visits' ">
                                                            <b>{{get_number_int(total_num_to_enter_compare,total_num_to_enter) | number:'1.0-2'}}</b><br>
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech,total_num_to_enter_compare ) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!-- tổng số lượng PasserBy -->
                                                        <td [hidden]="indexOptionSelected !=='PasserBy' ">
                                                            <b>{{total_passer_by  |  number:'1.0-2'}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='PasserBy' ">
                                                            <b>{{total_passer_by_compare > 0 ? ( total_passer_by_compare  |  number:'1.0-2') : 0}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='PasserBy' ">
                                                            <b>{{get_number_int(total_passer_by_compare,total_passer_by) | number:'1.0-2'}}</b><br>
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech,total_passer_by_compare ) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!-- tổng số lượng Kids Visits -->
                                                        <td [hidden]="indexOptionSelected !=='Kids Visits' ">
                                                            <b>{{total_kids  |  number:'1.0-2'}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Kids Visits' ">
                                                            <b>{{total_kids_compare > 0 ? ( total_kids_compare  |  number:'1.0-2') : 0}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Kids Visits' ">
                                                            <b>{{get_number_int(total_kids_compare,total_kids) | number:'1.0-2'}}</b><br>
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech,total_kids_compare ) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!-- tổng số lượng Turn in rate -->
                                                        <td [hidden]="indexOptionSelected !=='Turn in rate' ">
                                                            <b>{{total_turn_rate  |  number:'1.0-2'}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Turn in rate' ">
                                                            <b>{{total_turn_rate_compare > 0 ? ( total_turn_rate_compare  |  number:'1.0-2') : 0}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Turn in rate' ">
                                                            <b>{{get_number_int(total_turn_rate_compare,total_turn_rate) | number:'1.0-2'}}</b><br>
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech,total_turn_rate_compare ) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!-- tổng số lượng Shopper Visits -->
                                                        <td [hidden]="indexOptionSelected !=='Shopper Visits' ">
                                                            <b>{{total_shopper_visits  |  number:'1.0-2'}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Shopper Visits' ">
                                                            <b>{{total_shopper_visits_compare > 0 ? ( total_shopper_visits_compare  |  number:'1.0-2') : 0}}</b><br>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Shopper Visits' ">
                                                            <b>{{get_number_int(total_shopper_visits_compare,total_shopper_visits) | number:'1.0-2'}}</b><br>
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech,total_shopper_visits_compare ) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>

                                                        <!-- tổng số lượng traffic -->
                                                        <td [hidden]="indexOptionSelected !=='Traffic Flow'">
                                                            <b>{{total_num_to_traffic |  number:'1.0-2'}}</b><br> <span class="spancell">Tổng Traffic flow</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Traffic Flow'">
                                                            <b>{{ total_num_to_traffic_compare > 0 ? (total_num_to_traffic_compare |  number:'1.0-2') : 0 }}</b><br>
                                                            <span class="spancell">Tổng Traffic flow được so sánh</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Traffic Flow'">
                                                            <b>{{get_number_int(total_num_to_traffic_compare,total_num_to_traffic) | number:'1.0-2'}}</b><br>
                                                            <span class="spancell">
                                                                ({{ get_compar_per(number_chenh_lech, total_num_to_traffic_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!-- time trung bình  -->
                                                        <td [hidden]="indexOptionSelected !=='Avg Time' ">
                                                            <b>{{toHHMMSS(total_avgtime)}}</b><br> <span class="spancell">Thời gian trung bình</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Avg Time' ">
                                                            <b>{{toHHMMSS(total_avgtime_compare)}}</b><br> <span class="spancell">Thời gian trung bình được so sánh</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Avg Time'">
                                                            <b>{{toHHMMSS(get_number_int(total_avgtime,total_avgtime_compare))}}</b><br>
                                                            <span class="spancell">Thời gian chênh lệch</span>
                                                        </td>
                                                    </tr>
                                                    <tr *ngFor="let item of chart_data">
                                                        <td style="padding-left: 2.5rem;">{{item.time_period}} </td>
                                                        <!-- hiển thị giá trị visit -->
                                                        <td [hidden]="indexOptionSelected !=='Visits' ">
                                                            {{ item.num_to_enter ? (item.num_to_enter |  number:'1.0-2') : '&minus; '}}
                                                            <span class="spancell">{{ item.num_to_enter ? '(' + Go_number(item.num_to_enter, total_num_to_enter) + '%)' : ''}}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Visits' ">
                                                            {{item.num_to_enter_compare ? (item.num_to_enter_compare |  number:'1.0-2') : '' }}
                                                            <span class="spancell"> {{ item.num_to_enter_compare ? '('+ Go_number(item.num_to_enter_compare, total_num_to_enter_compare)+'%)' : '&minus;'}}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Visits' ">
                                                            {{ get_number_int(item.num_to_enter_compare,item.num_to_enter) | number:'1.0-2' }}
                                                            <span class="spancell">
                                                                ({{ get_compar_per(number_chenh_lech , item.num_to_enter_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!--  hiển thị giá trị traffic -->
                                                        <td [hidden]="indexOptionSelected !=='Traffic Flow'">
                                                            {{ item.traffic ? (item.traffic |  number:'1.0-2')  : '&minus; '}}
                                                            <span class="spancell">{{ item.traffic ? '('+ Go_number(item.traffic, total_num_to_traffic) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Traffic Flow'">
                                                            {{item.traffic_compare ?  (item.traffic_compare  |  number:'1.0-2') : '&minus; '}}
                                                            <span class="spancell">{{ item.traffic_compare ? '(' + Go_number(item.traffic_compare, total_num_to_traffic_compare) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Traffic Flow'">
                                                            {{ get_number_int(item.traffic_compare,item.traffic) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.traffic_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!--  hiển thị giá trị PasserBy -->
                                                        <td [hidden]="indexOptionSelected !=='PasserBy'">
                                                            {{ item.passer_by ? (item.passer_by |  number:'1.0-2')  : '&minus; '}}
                                                            <span class="spancell">{{ item.passer_by ? '('+ Go_number(item.passer_by, total_passer_by) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='PasserBy'">
                                                            {{item.passer_by_compare ?  (item.passer_by_compare  |  number:'1.0-2') : '&minus; '}}
                                                            <span class="spancell">{{ item.passer_by_compare ? '(' + Go_number(item.passer_by_compare, total_passer_by_compare) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='PasserBy'">
                                                            {{ get_number_int(item.passer_by_compare,item.passer_by) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.passer_by_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!--  hiển thị giá trị Kids Visits -->
                                                        <td [hidden]="indexOptionSelected !=='Kids Visits'">
                                                            {{ item.kids_visits ? (item.kids_visits |  number:'1.0-2')  : '&minus; '}}
                                                            <span class="spancell">{{ item.kids_visits ? '('+ Go_number(item.kids_visits, total_kids) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Kids Visits'">
                                                            {{item.kids_visits_compare ?  (item.kids_visits_compare  |  number:'1.0-2') : '&minus; '}}
                                                            <span class="spancell">{{ item.kids_visits_compare ? '('+ Go_number(item.kids_visits_compare, total_kids_compare) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Kids Visits'">
                                                            {{ get_number_int(item.kids_visits_compare,item.kids_visits) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.kids_visits_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!--  hiển thị giá trị Turn in rate -->
                                                        <td [hidden]="indexOptionSelected !=='Turn in rate'">
                                                            {{ item.turn_in_rate ? (item.turn_in_rate |  number:'1.0-2')  : '&minus; '}}
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Turn in rate'">
                                                            {{item.turn_in_rate_compare ?  (item.turn_in_rate_compare  |  number:'1.0-2') : '&minus; '}}
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Turn in rate'">
                                                            {{ get_number_int(item.turn_in_rate_compare,item.turn_in_rate) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.turn_in_rate_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!--  hiển thị giá trị Shopper Visits -->
                                                        <td [hidden]="indexOptionSelected !=='Shopper Visits'">
                                                            {{ item.shopper_visits ? (item.shopper_visits |  number:'1.0-2')  : '&minus; '}}
                                                            <span class="spancell">{{ item.shopper_visits ? '('+ Go_number(item.shopper_visits, total_shopper_visits) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Shopper Visits'">
                                                            {{item.shopper_visits_compare ?  (item.shopper_visits_compare  |  number:'1.0-2') : '&minus; '}}
                                                            <span class="spancell">{{ item.shopper_visits_compare ? '(' + Go_number(item.shopper_visits_compare, total_shopper_visits_compare) + '%)' : '' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Shopper Visits'">
                                                            {{ get_number_int(item.shopper_visits_compare,item.shopper_visits) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.shopper_visits_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>

                                                        <!-- hiển thị thời gian -->
                                                        <td class="{{item.avg_time  ? '' : 'td_center'}}" [hidden]="indexOptionSelected !=='Avg Time' ">
                                                            <span>{{item.avg_time ? toHHMMSS(item.avg_time *60) :  '&minus; ' }}</span>
                                                        </td>
                                                        <td class="{{item.avg_time_compare  ? '' : 'td_center'}}" [hidden]="indexOptionSelected !=='Avg Time' ">
                                                            <span>{{item.avg_time_compare ? toHHMMSS(item.avg_time_compare *60) : '&minus; ' }}</span>
                                                        </td>
                                                        <td [hidden]="indexOptionSelected !=='Avg Time'">
                                                            {{ toHHMMSS( (get_number_int(item.avg_time_compare, item.avg_time)) * 60) }}
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div><!--  table-repopnsive -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="divbaocao" style="float: right; font-size: 11px">Báo cáo này đã được tạo vào {{time_generate_report | date:
            'dd/MM/yyyy'}} lúc {{time_generate_report | date: 'hh:mm:ss'}} -
            <span style="color:blue; cursor:pointer;" (click)="reset_session()">làm mới báo cáo</span>
        </div>
    </div>
</div>