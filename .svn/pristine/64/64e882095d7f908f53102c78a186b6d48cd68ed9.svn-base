                                 <tr *ngFor="let item of all_data">
                                    <ng-container *ngIf="indexOptionSelected !== 'Avg Time'">
                                       <td style="padding-left: 2.5rem;">{{item.time_period}}</td>
                                       <td> {{item.data | number:'1.0-2'}} <span [hidden]="indexOptionSelected === 'Turn in rate'  ||  indexOptionSelected === 'Conversion rate' || indexOptionSelected === 'Loyal Visits' || indexOptionSelected === 'Loyal Transactions' || indexOptionSelected === 'Loyal Conversion rate' || indexOptionSelected === 'Missed Loyal rate' || indexOptionSelected === 'CX index' || 
                                          indexOptionSelected === 'Sales hours' ||     indexOptionSelected === 'Sales on sales hour' || indexOptionSelected === 'Shoppers on sales hour' || indexOptionSelected === 'NPS index' || indexOptionSelected === 'ATV' || indexOptionSelected === 'Avg Items'" class="spancell">({{item.data_percent}}%)</span> </td>
                                       <td> {{item.data_compare | number:'1.0-2'}} <span [hidden]="indexOptionSelected === 'Turn in rate'  ||  indexOptionSelected === 'Conversion rate' || indexOptionSelected === 'Loyal Visits' || indexOptionSelected === 'Loyal Transactions' || indexOptionSelected === 'Loyal Conversion rate' || indexOptionSelected === 'Missed Loyal rate' || indexOptionSelected === 'CX index' ||   indexOptionSelected === 'Sales hours' ||     indexOptionSelected === 'Sales on sales hour' || indexOptionSelected === 'Shoppers on sales hour' || indexOptionSelected === 'NPS index' || indexOptionSelected === 'ATV' || indexOptionSelected === 'Avg Items'" class="spancell">({{item.data_compare_percent}}%)</span> </td>
                                       <td class="{{item.data > item.data_compare ? 'fa-icon-green' : item.data == item.data_compare ? 'fa-icon-gray': 'fa-icon-red'  }}">
                                          {{get_number_int(item.number_compare) | number:'1.0-2'}}
                                       </td>
                                    </ng-container>
                                    <ng-container *ngIf="indexOptionSelected === 'Avg Time'">
                                       <td style="padding-left: 2.5rem;">{{item.time_period}}</td>
                                       <td> {{ item.data ? (toHHMMSS(item.data *60)) : '00:00:00' }} </td>
                                       <td> {{ item.data_compare ? (toHHMMSS(item.data_compare *60)) : '00:00:00' }} </td>
                                       <td> {{item.data >= item.data_compare ? toHHMMSS((item.data - item.data_compare)*60 ) : toHHMMSS((item.data_compare - item.data)*60) }} </td>
                                    </ng-container>
                                 </tr>
                              </tbody>
                           </table>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <div id="divbaocao" style="float: right; font-size: 11px">Báo cáo này đã được tạo vào {{time_generate_report | date:
         'dd/MM/yyyy'}} lúc {{time_generate_report | date: 'hh:mm:ss'}} -
         <span style="color:blue; cursor:pointer;" (click)="reset_session()">làm mới báo cáo</span>
      </div>
   </div>
</div>