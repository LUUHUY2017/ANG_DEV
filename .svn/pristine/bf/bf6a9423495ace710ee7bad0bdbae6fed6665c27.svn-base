                                                        <!--  hiển thị giá trị CX index -->
                                                        <td [hidden]="indexOptionSelected !=='CX index'"> {{ item.cx_index ? (item.cx_index |  number:'1.0-2')  : '&minus; '}} </td>
                                                        <td [hidden]="indexOptionSelected !=='CX index'"> {{item.cx_index_compare ? (item.cx_index_compare  |  number:'1.0-2') : '&minus; '}} </td>
                                                        <td [hidden]="indexOptionSelected !=='CX index'"> {{ get_number_int(item.cx_index_compare,item.cx_index) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.cx_index_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                        <!--  hiển thị giá trị NPS index -->
                                                        <td [hidden]="indexOptionSelected !=='NPS index'"> {{ item.nps_index ? (item.nps_index |  number:'1.0-2')  : '&minus; '}} </td>
                                                        <td [hidden]="indexOptionSelected !=='NPS index'"> {{item.nps_index_compare ?  (item.nps_index_compare  |  number:'1.0-2') : '&minus; '}} </td>
                                                        <td [hidden]="indexOptionSelected !=='NPS index'"> {{ get_number_int(item.nps_index_compare,item.nps_index) | number:'1.0-2' }}
                                                            <span class="spancell"> ({{ get_compar_per(number_chenh_lech, item.nps_index_compare) | number:'1.0-2' }}%)
                                                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                                                            </span>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="divbaocao" style="float: right; font-size: 11px">{{language.bao_cao_duoc_tao}} {{time_generate_report | date:  'dd/MM/yyyy'}} {{language.luc}} {{time_generate_report | date: 'hh:mm:ss'}} -
                <span style="color:blue; cursor:pointer;" (click)="reset_session()">{{language.lam_moi_bao_cao}}</span>
          </div>
    </div>
