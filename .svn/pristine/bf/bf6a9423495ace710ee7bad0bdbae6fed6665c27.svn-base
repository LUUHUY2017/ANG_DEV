<block-ui></block-ui>
<div class="card card-parent">
    <div class="card-header">
        <div class="row mr-0 ml-0" id="time-comparison-header">
            <div class="col-sm-12 col-md-4 p-0">
                <app-menutree (submit_menu)="get_emit_menu($event)"></app-menutree>
            </div>
            <div class="col-sm-12 col-md-8 p-0">
                <div class="time-period-comparison">
                    <app-timeperiod class="time_input1 time_no_input1 input_no_time1" #TimeInput (submit_date)="get_date_from_emit($event,1)">
                    </app-timeperiod>
                    <span style="padding: 0px 10px;font-size:11px;">{{language.so_voi}}</span>
                    <app-timeperiod class="time_input2 time_no_input2 input_no_time2" #TimeInput_2 (submit_date)="get_date_from_emit($event,2)">
                    </app-timeperiod>
                </div>
            </div>
        </div>
    </div> <!-- end-card-header -->
    <div class="card-body card-body-bottom">
        <div class="row">
            <div class="container-fluid">
                <script src="ace-builds/src-min/ace.js"></script>
                <div class="animated fadeIn">
                    <div class="card card-child">
                        <div class="card-header">
                            <div class="row">
                                <ul id="headmapmenu_2">
                                    <li style="margin-right:5px;">
                                        <ng-select class="bootstrap selecttimeview select-visits" [items]="indexOption" bindLabel="label" bindValue="value" groupBy="group" [clearable]=false [(ngModel)]="indexOptionSelected"></ng-select>
                                    </li>
                                    <li>
                                        <div class="btn-group btn-group-toggle ml" data-toggle="buttons">
                                            <ng-select class="bootstrap selecttimeview starttime" [items]="startTimeOption" bindLabel="label" bindValue="value" [clearable]=false [(ngModel)]="start_time"></ng-select>
                                            <span style="padding: 3px 0;">-</span>
                                            <ng-select class="bootstrap selecttimeview endtime" [items]="endTimeOption" bindLabel="label" bindValue="value" [clearable]=false [(ngModel)]="end_time"></ng-select>
                                        </div>
                                        <button type="button" class="btn btn-primary btnapdung" [disabled]="btnApplyValid" (click)="get_data()">
                                                {{language.ap_dung}}
                                        </button>
                                    </li>
                                    <li>
                                        <div>
                                            <div class="btn-group float-right" dropdown>
                                                <button dropdownToggle type="button" class="btn btn-primary dropdown-toggle float-right btnxuat"> {{language.xuat}}
                                                </button>
                                                <ul *dropdownMenu class="dropdown-menu float-right" role="menu">
                                                    <li role="menuitem"><a style="cursor:pointer" class="dropdown-item float-right" (click)="exportExcel()">{{language.xuat_ra_excel}}</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
