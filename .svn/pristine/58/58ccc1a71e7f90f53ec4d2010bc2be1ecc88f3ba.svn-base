.customer_filter_table ul {
	width: 100%;
	padding: 3px 15px 0 15px;
	height: 30px;
	margin-bottom: 0px;
}

.customer_filter_table li {
	list-style: none;
}

.customer_filter_table li>a {
	color: #20a8d8;
	cursor: pointer;
}

.customer_filter_table li>a:hover {
	text-decoration: underline;
