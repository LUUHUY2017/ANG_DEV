.btn {
	transition: unset !important;
}

.cursor-pointer {
	cursor: pointer;
}

/*----------  card header  ----------*/

#fba-ana-head {
	padding: 0 7px;
	margin-bottom: 0px;
	width: 100%;
}

#fba-ana-head li {
	list-style: none;
	position: relative;
}

#fba-ana-head>li .text-select {
	font-size: 14px;
	font-weight: 700;
	line-height: 24px;
	display: inline-block;
}

#fba-ana-head>li>label {
	font-weight: normal;
	line-height: 14px;
	margin-bottom: 0;
	color: #7b7b7b;
	font-size: 12px;
}

#fba-ana-head>li>input {
	padding-left: 3px;
	margin-top: 15px;
}

#fba-ana-head>li:nth-child(1) {
	width: 21%;
	text-align: left;
	float: left;
	margin-right: 12%;
}

#fba-ana-head>li:nth-child(2) {
	width: 41%;
	text-align: left;
	float: left;
}

#fba-ana-head>li:nth-child(3) {
	width: auto;
	float: right;
}

/*----------  li > div.card header  ----------*/

.overview-survey, .overview-period, .overview-location {
	cursor: pointer;
}

/*----------  Phần câu hỏi  ----------*/

#fba-ana-head .khung-question {
	position: absolute;
	left: 0;
	min-width: 320px;
	height: 180px;
	overflow: hidden;
	z-index: 10;
	background: white;
	box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14), 0px 3px 14px 2px rgba(0, 0, 0, 0.12) !important;
}

#fba-ana-head .khung-question #khung-question-scroll {
	padding: 0 0 8px 8px;
	height: 100%;
	overflow-y: auto;
}

#fba-ana-head .khung-question #khung-question-scroll .timePeriod span {
	display: block;
}

/* Thnah scrollbar question */

/* #fba-ana-head .khung-question #khung-question-scroll::-webkit-scrollbar {
	width: 4px;
	background-color: darkgray;
} */

#fba-ana-head .khung-question .small-metric {
	font-size: 10px;
	border-bottom: 1px solid #e1e2e1;
	font-style: italic;
	font-weight: bold;
	padding: 7px 0 3px 0px;
}

#fba-ana-head .khung-question #khung-question-scroll li {
	/*border-bottom: 1px solid #e1e2e1;*/
	cursor: pointer;
	padding: 8px 5px 5px 10px;
}

#fba-ana-head .khung-question .questionText {
	color: forestgreen;
	font-weight: bold;
	font-size: 13px;
}

#fba-ana-head .khung-question .timePeriod {
	font-size: 10px;
}

/*----------  Phần địa điểm  ----------*/

#fba-ana-head .khung-location {
	position: absolute;
	left: 0;
	width: 270px;
	border-radius: 3px;
	height: 220px;
	overflow: hidden;
	background: white;
	z-index: 10;
	box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14), 0px 3px 14px 2px rgba(0, 0, 0, 0.12) !important;
}

#fba-ana-head .khung-location #khung-location-scroll {
	overflow-y: scroll;
	height: 100%;
}

/* Thanh scrollbar location*/

/*#fba-ana-head .khung-location::-webkit-scrollbar {
	width: 4px;
	height: 8px;
	background-color: darkgray;
}*/

ul#ul-location {
	padding: 0;
	margin: 0;
}

#fba-ana-head .khung-location li {
	padding: 8px 0 8px 8px;
	/*border-bottom: 1px solid #e1e2e1;*/
	cursor: pointer;
	padding-left: 10px;
	list-style: none;
}

#fba-ana-head .khung-location>input {
	padding: 20px 8px 8px 8px;
}

#fba-ana-head .khung-location input:focus {
	color: #5c6873;
	background-color: #fff;
	border-color: none;
	outline: 0;
	box-shadow: 0 0 0 0.2rem rgba(255, 255, 255, 0.25);
}

#fba-ana-head .khung-location>li:hover {
	background-color: rgba(0, 0, 0, 0.08);
}

.location {
	font-size: 17px;
}

.khung-location .menu-0 {
	width: 100%;
}

.khung-location .menu-1 {
	margin-left: 25px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 25px)!important;
}

.khung-location .menu-2 {
	margin-left: 35px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 35px)!important;
}

.khung-location .menu-3 {
	margin-left: 45px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 45px)!important;
}

.khung-location .menu-4 {
	margin-left: 55px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 55px)!important;
}

.khung-location .menu-5 {
	margin-left: 65px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 65px)!important;
}

.khung-location .menu-6 {
	margin-left: 75px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 75px)!important;
}

/*---------- Phần thời kỳ  ----------*/

#fba-ana-head .khung-time {
	position: absolute;
	right: -15%;
	width: 205px;
	border-radius: 3px;
	height: 270px;
	overflow: hidden;
	background: white;
	z-index: 10;
	box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14), 0px 3px 14px 2px rgba(0, 0, 0, 0.12) !important;
}

#khung-time-scroll #ul-time li.active {
	color: #2bb540;
}

#fba-ana-head .khung-time #khung-time-scroll {
	height: 100%;
	overflow-y: scroll;
}

/* Thanh scrollbar time*/

/*#fba-ana-head .khung-time::-webkit-scrollbar {
	width: 4px;
	height: 8px;
	background-color: darkgray;
}*/

#fba-ana-head .khung-time li {
	padding: 8px 0 8px 8px;
	/*border-bottom: 1px solid #e1e2e1;*/
	cursor: pointer;
	padding-left: 10px;
	list-style: none;
}

#fba-ana-head .khung-time li:hover {
	background-color: rgba(0, 0, 0, 0.08);
}

ul#ul-time {
	padding: 0;
	margin: 0;
}

