<notifier-container></notifier-container>
<div class="row">
    <div class="col-md-12 float-right">
        <div style="text-align: right;width: 100%;font-size: 11px;">
            Báo cáo này đã được tạo vào {{ time_generate_report | date: "dd/MM/yyyy" }} lúc {{ time_generate_report | date: "hh:mm:ss" }} -
            <span (click)="reset_session()" style=" color: blue; cursor: pointer;">làm mới báo cáo</span>
        </div>
    </div>
    <!-- Modal 1  -->
    <ng-template #template1>
        <div class="modal-header">
            <h5 class="modal-title pull-left"><i class="fa fa-cog"></i> Cấu hình</h5>
            <button type="button" class="close pull-right" aria-label="Close" (click)="modalRef.hide()">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body bg-modal-body">
            <ng-container *ngIf="userinfo.organization_id === '0'">
                <ng-select class="bootstrap selecttimeview  ng-select0" [items]="organization_array" bindLabel="label" bindValue="value" [clearable]=false (change)="change_organization($event, 1)" [(ngModel)]="organization_id1"></ng-select>
            </ng-container>
            <!-- Textbox chọn -->
            <div class="config-text-dashboard">
                <div style="display:block;border:1px solid #ddd; padding:5px;background:white" (click)="siteSelectionDisplay = !siteSelectionDisplay" class="text-select vung-kich">
                    <span>&nbsp;{{ location1 }} <i class="icon-down"> ▼ </i></span>
                </div>
            </div>
            <!-- Chọn địa điểm/ khu vực -->
            <div [hidden]="!siteSelectionDisplay" class=" khung-location-dashboard block" (clickOutside)="siteSelectionDisplay = false" [exclude]="'.vung-kich'">
                <div id="khung-location-scroll">
                    <div style="position: relative;width: 100%;" class="inputgroup">
                        <input (keyup)="search_menu($event.target.value, 1)" style="border:none !important;padding-left: 7px" type="text" class="form-control" placeholder="Tìm kiếm địa điểm/ khu vực" />
                        <i style="position: absolute;top: 11px; right:8px; color: #e1e2e3; font-size: 15px;" class="fa fa-search" aria-hidden="true"></i>
                    </div>
                    <ul style="padding-left: 0;">
                        <li *ngFor="let item of menu_tree1" (click)="changeitem(1, item.site_name, item.id)" class="menu-{{ item.alevel }} {{item.site_name === location1 ? 'active' : '' }} ">
                            {{ item.site_name }}
                        </li>
                    </ul>
                </div>
            </div>
            <!-- Khung thời gian -->
            <div class="config-text-dashboard">
                <div style="display:block;border:1px solid #ddd; padding:5px;background:white;" (click)="siteSelectionDisplay3 = !siteSelectionDisplay3" class="text-select vung-kich2">
                    <span>&nbsp;{{ time_period1 }} <i class="icon-down"> ▼ </i></span>
                </div>
            </div>
            <div [hidden]="!siteSelectionDisplay3" (clickOutside)="siteSelectionDisplay3 = false" [exclude]="'.vung-kich2'" class=" khung-location-dashboard block">
                <div id="khung-location-scroll">
                    <ul id="ul-time-dashboard">
                        <li *ngFor="let item of fba_time_period_overview" (click)="chonthoigian(1, item)" class="{{ item.label === time_period1 ? 'active' : '' }} ">
                            {{ item.label }}
                        </li>
                    </ul>
                </div>
            </div>
            <ng-select class="bootstrap selecttimeview  ng-select1" [items]="startTimeOption" bindLabel="label" bindValue="value" [clearable]=false [(ngModel)]="startTime1"></ng-select>
            <ng-select class="bootstrap selecttimeview  ng-select2" [items]="endTimeOption" bindLabel="label" bindValue="value" [clearable]=false [(ngModel)]="endTime1"></ng-select>
            <ng-select class="bootstrap selecttimeview  ng-select3" [items]="select_visit_option" bindLabel="label" groupBy="group" bindValue="value" [clearable]=false [(ngModel)]="selected_show1"></ng-select>
        </div>
        <div class="modal-footer">
            <a (click)="modalRef.hide()" class="btn  btn-huy active"> Đóng</a>
            <button class="btn  btn-primary" (click)="load_data_box1()">
                Áp dụng
            </button>
        </div>
    </ng-template>
