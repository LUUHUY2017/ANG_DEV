.khungthemmoi
{
    /* font-weight: bold; */
    font-family: Roboto, sans-serif;
}

.card
{
    border-radius: 0px;
}
.body-breadcumb
{
    padding: 10px 30px 10px 30px;
}
.khungthemmoi ul
{
    width: 100%;
    padding-left: 0px;
    margin-bottom: 0px;
    font-family: -apple-system,BlinkMacSystemFont,segoe ui,Roboto,helvetica neue,Arial,sans-serif,apple color emoji,segoe ui emoji,segoe ui symbol,noto color emoji;
}
.khungthemmoi li
{
    list-style: none;
}
.khungthemmoi li>a
{
    color: #20a8d8;
    cursor: pointer;
}
.khungthemmoi li>a:hover
{
    text-decoration: underline;
}
.khungthemmoi li:nth-child(1)
{
    float: left;
}
.khungthemmoi li:nth-child(2)
{
    float: right;
}

.body-content .card-body
{
    padding: 10px 20px;
}
#showphantu select
{
    padding: 0px 5px;
    border-radius: 4px;
    border-color: #ddd;
}
.input-text
{
    border-radius: 3px;
    border-color: #ddd;
    border-style: double;
    padding: 2px 0px 2px 5px;
}
table .fa-remove
{
    color: darkred;
    cursor: pointer;
}
.input-text:focus
{
    border-radius: 2px;
}
.no-margin
{
    margin: 0px;
}
.edit-button
{
    border-radius: 0px;
    font-family: Roboto, sans-serif;
}
#edit-table
{
    margin-bottom: 5px;
    /* min-width: 900px; */
    /* width: 900px; */
}
.edit-group-button li:last-child a
{
    border-radius: 1px;
}
.edit-group-button li:first-child a
{
    border-radius: 1px;
}
.table-prime .ui-paginator
{
    border: none;
    background: none;
    float: right;
    margin-top: 10px;
}
.table-prime th i
{
    font-size: 14px;
}