                              {{ item.loyal_visits ? (item.loyal_visits |  number:'1.0-2')  : '&minus; '}} <span
                                class="spancell">(%)</span> </td>
                            <td [hidden]="indexOptionSelected !==indexes.member_visitors">
                              {{item.loyal_visits_compare ? (item.loyal_visits_compare  |  number:'1.0-2') : '&minus; '}}
                              <span class="spancell">(%)</span>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.member_visitors">
                              {{ get_number_int(item.loyal_visits_compare,item.loyal_visits) | number:'1.0-2' }}
                              <span class="spancell">
                                ({{ get_compar_per(number_chenh_lech, item.loyal_visits_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </span>
                            </td>
                            <!--  hiển thị giá trị Loyal Transactions -->
                            <td [hidden]="indexOptionSelected !==indexes.member_transactions">
                              {{ item.loyal_transactions ? (item.loyal_transactions |  number:'1.0-2')  : '&minus; '}}
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.member_transactions">
                              {{item.loyal_transactions_compare ? (item.loyal_transactions_compare  |  number:'1.0-2') : '&minus; '}}
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.member_transactions">
                              {{ get_number_int(item.loyal_transactions_compare,item.loyal_transactions) | number:'1.0-2' }}
                              <span class="spancell">
                                ({{ get_compar_per(number_chenh_lech, item.loyal_transactions_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </span>
                            </td>
                            <!--  hiển thị giá trị Loyal Conversion rate -->
                            <td [hidden]="indexOptionSelected !==indexes.member_conversion_rate">
                              {{ item.loyal_conversion ? (item.loyal_conversion |  number:'1.0-2')  : '&minus; '}} <span
                                class="spancell">(%)</span> </td>
                            <td [hidden]="indexOptionSelected !==indexes.member_conversion_rate">
                              {{item.loyal_conversion_compare ? (item.loyal_conversion_compare  |  number:'1.0-2') : '&minus; '}}
                              <span class="spancell">(%)</span>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.member_conversion_rate">
                              {{ get_number_int(item.loyal_conversion_compare,item.loyal_conversion) | number:'1.0-2' }}
                              <span class="spancell">
                                ({{ get_compar_per(number_chenh_lech, item.loyal_conversion_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </span>
                            </td>
                            <!--  hiển thị giá trị Missed Loyal rate -->
                            <td [hidden]="indexOptionSelected !==indexes.missed_member_rate">
                              {{ item.loyal_conversion > 0  ? (( 100 - item.loyal_conversion) |  number:'1.0-2')  : '&minus; '}}<span
                                class="spancell">(%)</span>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.missed_member_rate">
                              {{item.missed_loyal_compare ? (item.missed_loyal_compare  |  number:'1.0-2') : '&minus; '}}<span
                                class="spancell">(%)</span>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.missed_member_rate">
                              {{ get_number_int(item.missed_loyal_compare,item.loyal_conversion > 0  ? (( 100 - item.loyal_conversion) |  number:'1.0-2') :0) | number:'1.0-2' }}
                              <span class="spancell">
                                ({{ get_compar_per(number_chenh_lech, item.missed_loyal_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </span>
                            </td>
                            <!--  hiển thị giá trị CX Index -->
                            <td [hidden]="indexOptionSelected !==indexes.cx_index">
                              {{ item.cx_index ? (item.cx_index |  number:'1.0-2')  : '&minus; '}} <span
                                class="spancell">(%)</span> </td>
                            <td [hidden]="indexOptionSelected !==indexes.cx_index">
                              {{item.cx_index_compare ? (item.cx_index_compare  |  number:'1.0-2') : '&minus; '}} <span
                                class="spancell">(%)</span></td>
                            <td [hidden]="indexOptionSelected !==indexes.cx_index">
                              {{ get_number_int(item.cx_index_compare,item.cx_index) | number:'1.0-2' }}
                              <span class="spancell">
                                ({{ get_compar_per(number_chenh_lech, item.cx_index_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </span>
                            </td>
                            <!--  hiển thị giá trị{{indexes.nps_index}} -->
                            <td [hidden]="indexOptionSelected !==indexes.nps_index">
                              {{ item.nps_index ? (item.nps_index |  number:'1.0-2')  : '&minus; '}}<span
                                class="spancell">(%)</span> </td>
                            <td [hidden]="indexOptionSelected !==indexes.nps_index">
                              {{item.nps_index_compare ?  (item.nps_index_compare  |  number:'1.0-2') : '&minus; '}}<span
                                class="spancell">(%)</span>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.nps_index">
                              {{ get_number_int(item.nps_index_compare,item.nps_index) | number:'1.0-2' }}
                              <span class="spancell">
                                ({{ get_compar_per(number_chenh_lech, item.nps_index_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </span>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="divbaocao" style="float: right; font-size: 11px">{{language.bao_cao_duoc_tao}}
      {{time_generate_report | date:  'dd/MM/yyyy'}} {{language.luc}} {{time_generate_report | date: 'hh:mm:ss'}} -
      <span style="color:blue; cursor:pointer;" (click)="reset_session()">{{language.lam_moi_bao_cao}}</span>
    </div>
  </div>
</div>


<ng-template #content let-c="close" let-d="dismiss">
  <div class="modal-header">
    <h4 class="modal-title" id="modal-basic-title">Thông báo!</h4>
    <button type="button" class="close" aria-label="Close" (click)="modalRef.hide()">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="modal-body">
    <p>Có lỗi xảy ra! Vui lòng nhấn<a style="color: blue; cursor: pointer;" (click)="reset_session()"> vào đây </a> để
      tải lại</p>
  </div>
  <div class="modal-footer">
    <button type="button" class="btn btn-outline-dark" (click)="reset_session()">Tải lại</button>
  </div>
</ng-template>
