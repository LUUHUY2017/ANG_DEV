                            <!-- tổng số lượng Sales -->
                            <td [hidden]="indexOptionSelected !=='Sales'"><b>{{tt_sales |  number:'1.0-2'}}</b></td>
                            <td [hidden]="indexOptionSelected !=='Sales'">
                              <b>{{ tt_sales_compare > 0 ? (tt_sales_compare |  number:'1.0-2') : 0 }}</b></td>
