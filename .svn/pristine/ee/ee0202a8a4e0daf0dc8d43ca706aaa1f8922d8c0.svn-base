<block-ui></block-ui>
<div class="card card-parent">
  <div class="card-header">
    <div class="row mr-0 ml-0" id="time-comparison-header">
      <div class="col-sm-12 col-md-4 p-0">
        <app-menutree (submit_menu)="get_emit_menu($event)"></app-menutree>
      </div>
      <div class="col-sm-12 col-md-8 p-0">
        <div class="time-period-comparison">
          <app-timeperiod class="time_input1 time_no_input1 input_no_time1" #TimeInput
            (submit_date)="get_date_from_emit($event,1)">
          </app-timeperiod>
          <span style="padding: 0px 10px;font-size:11px;">{{language.so_voi}}</span>
          <app-timeperiod2 class="time_input2 time_no_input2" #TimeInput_2
            (submit_date2)="get_date_from_emit($event,2)">
          </app-timeperiod2>
        </div>
      </div>
    </div>
  </div> <!-- end-card-header -->
  <div class="card-body card-body-bottom">
    <div class="row">
      <div class="container-fluid">
        <script src="ace-builds/src-min/ace.js"></script>
        <div class="animated fadeIn">
          <div class="card card-child">
            <div class="card-header">
              <div class="row">
                <ul id="headmapmenu_2">
                  <li style="margin-right:5px;">
                    <ng-select class="bootstrap selecttimeview select-visits" [items]="indexOption" bindLabel="label"
                      bindValue="value" groupBy="group" [clearable]=false [(ngModel)]="indexOptionSelected"></ng-select>
                  </li>
                  <li>
                    <div class="btn-group btn-group-toggle ml" data-toggle="buttons">
                      <ng-select class="bootstrap selecttimeview starttime" [items]="startTimeOption" bindLabel="label"
                        bindValue="value" [clearable]=false [(ngModel)]="start_time"></ng-select>
                      <span style="padding: 3px 0;">-</span>
                      <ng-select class="bootstrap selecttimeview endtime" [items]="endTimeOption" bindLabel="label"
                        bindValue="value" [clearable]=false [(ngModel)]="end_time"></ng-select>
                    </div>
                    <button type="button" class="btn btn-primary btnapdung" [disabled]="btnApplyValid"
                      (click)="get_data()">
                      {{language.ap_dung}}
                    </button>
                  </li>
                  <li>
                    <div>
                      <div class="btn-group float-right" dropdown>
                        <button dropdownToggle type="button"
                          class="btn btn-primary dropdown-toggle float-right btnxuat"> {{language.xuat}}
                        </button>
                        <ul *dropdownMenu class="dropdown-menu float-right" role="menu">
                          <li role="menuitem"><a style="cursor:pointer" class="dropdown-item float-right"
                              (click)="exportExcel()">{{language.xuat_ra_excel}}</a></li>
                        </ul>
                      </div>
                    </div>
                  </li>
                </ul>
              </div>
            </div> <!-- card-header -->
            <div class="card-body card-body-bottom">
              <div class="row">
                <div class="col-sm-5">
                </div>
                <div class="col-sm-7 d-none d-md-block">
                  <div class="btn-group btn-group-toggle float-right " data-toggle="buttons">
                    <label class="btn btn-custom btn-outline-secondary" [(ngModel)]="viewDataBy" btnRadio="Hour"
                      (click)="change_view()">{{language.gio}}</label>
                    <label *ngIf="index_viewby >= 2" class="btn btn-custom btn-outline-secondary"
                      [(ngModel)]="viewDataBy" btnRadio="Day" (click)="change_view()">{{language.ngay}}</label>
                    <label *ngIf="index_viewby >= 3" class="btn btn-custom btn-outline-secondary"
                      [(ngModel)]="viewDataBy" btnRadio="Week" (click)="change_view()">{{language.tuan}}</label>
                    <label *ngIf="index_viewby >= 4" class="btn btn-custom btn-outline-secondary"
                      [(ngModel)]="viewDataBy" btnRadio="Month" (click)="change_view()">{{language.thang}}</label>
                    <label *ngIf="index_viewby >= 5" class="btn btn-custom btn-outline-secondary"
                      [(ngModel)]="viewDataBy" btnRadio="Year" (click)="change_view()">{{language.nam}}</label>
                  </div>
                </div>
              </div>
              <div class="row foot-title-show"> {{site_name}} &nbsp; {{ title_index }} &nbsp; {{ starttime }}
                &nbsp;&minus;&nbsp; {{ endtime }} &nbsp; {{ title_time_period }}: {{ time_period }} &nbsp; <i
                  style="border-bottom: 1px dotted #7b7b7b;"> {{language.so_voi}} </i> &nbsp; {{time_period_compare}}
              </div>
              <div class="row">
                <div class="col-sm-12 col-md-12 ">
                  <div #container1></div>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-12 col-md-12 ">
                  <div style="border-top:1px dotted #a7a4a4" class="row">
                    <div class="table-responsive">
                      <table style="width: 100%;min-width: 900px" id="table"
                        class="table table-striped table-comparation">
                        <thead>
                          <tr>
                            <th style="width:220px;">{{language.time}}
                              <br> <span style="margin-left:0%;" class="spancell"> ({{ show_label_table }})
                              </span></th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.visitors ">
                              {{indexes.visitors}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.visitors ">
                              {{indexes.visitors}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.traffic_flow">
                              {{indexes.traffic_flow}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.traffic_flow">
                              {{indexes.traffic_flow}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.passerby">
                              {{indexes.passerby}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.passerby">
                              {{indexes.passerby}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.shoppers">
                              {{indexes.shoppers}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.shoppers">
                              {{indexes.shoppers}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.kids_visitors">
                              {{indexes.kids_visitors}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.kids_visitors">
                              {{indexes.kids_visitors}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.turn_in_rate">
                              {{indexes.turn_in_rate}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.turn_in_rate">
                              {{ remove_percent(indexes.turn_in_rate)}} Compared (%)</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.avg_time ">
                              {{indexes.avg_time}} (min) </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.avg_time ">
                              {{indexes.avg_time}} Compared (min)</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.conversion_rate">
                              {{indexes.conversion_rate}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.conversion_rate">
                              {{ remove_percent(indexes.conversion_rate)}} Compared (%)</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.atv"> {{indexes.atv}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.atv"> {{indexes.atv}}
                              Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.avg_items">
                              {{indexes.avg_items}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.avg_items">
                              {{indexes.avg_items}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales_yield">
                              {{indexes.sales_yield}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales_yield">
                              {{indexes.sales_yield}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.transactions">
                              {{indexes.transactions}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.transactions">
                              {{indexes.transactions}} Compared </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales"> {{indexes.sales}}
                            </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales"> {{indexes.sales}}
                              Compared</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.missed_sales_opportunity">
                              {{indexes.missed_sales_opportunity}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.missed_sales_opportunity">
                              {{indexes.missed_sales_opportunity}} Compared</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales_hours">
                              {{indexes.sales_hours}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales_hours">
                              {{indexes.sales_hours}} Compared</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.shoppers_on_sales_hour">
                              {{indexes.shoppers_on_sales_hour}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.shoppers_on_sales_hour">
                              {{indexes.shoppers_on_sales_hour}} Compared</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales_on_sales_hour">
                              {{indexes.sales_on_sales_hour}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.sales_on_sales_hour">
                              {{indexes.sales_on_sales_hour}} Compared</th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.member_visitors">
                              {{indexes.member_visitors}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.member_visitors">
                              {{ remove_percent(indexes.member_visitors)}} Compared (%) </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.member_transactions">
                              {{indexes.member_transactions}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.member_transactions">
                              {{remove_percent(indexes.member_transactions)}} Compared (%) </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.member_conversion_rate">
                              {{indexes.member_conversion_rate}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.member_conversion_rate">
                              {{remove_percent (indexes.member_conversion_rate)}} Compared (%) </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.missed_member_rate">
                              {{indexes.missed_member_rate}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.missed_member_rate">
                              {{ remove_percent(indexes.missed_member_rate)}} Compared (%) </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.cx_index">
                              {{indexes.cx_index}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.cx_index">
                              {{remove_percent(indexes.cx_index)}} Compared (%) </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.nps_index">
                              {{indexes.nps_index}} </th>
                            <th style="width:250px" [hidden]="indexOptionSelected !==indexes.nps_index">
                              {{ remove_percent(indexes.nps_index)}} Compared (%) </th>
                            <th style="width:250px;font-size:22px;line-height: 20px;">
                              <span class="fa-icon-green "><i class="fa fa-caret-up"></i></span>
                              <span class="fa-icon-red pdl-5"><i class="fa fa-caret-down"></i></span>
                            </th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <td></td>
                            <!-- tổng số lượng visit -->
                            <td [hidden]="indexOptionSelected !==indexes.visitors ">
                              <b>{{total_num_to_enter  |  number:'1.0-2'}}</b><br> </td>
                            <td [hidden]="indexOptionSelected !==indexes.visitors ">
                              <b>{{total_num_to_enter_compare > 0 ? ( total_num_to_enter_compare  |  number:'1.0-2') : 0}}</b>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.visitors ">
                              <b>{{get_number_int(total_num_to_enter_compare,total_num_to_enter) | number:'1.0-2'}}</b>
                              <div class="spancell">
                                ({{ get_compar_per(number_chenh_lech,total_num_to_enter_compare ) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </div>
                            </td>
                            <!-- tổng số lượng PasserBy -->
                            <td [hidden]="indexOptionSelected !==indexes.passerby ">
                              <b>{{total_passer_by  |  number:'1.0-2'}}</b> </td>
                            <td [hidden]="indexOptionSelected !==indexes.passerby ">
                              <b>{{total_passer_by_compare > 0 ? ( total_passer_by_compare  |  number:'1.0-2') : 0}}</b>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.passerby ">
                              <b>{{get_number_int(total_passer_by_compare,total_passer_by) | number:'1.0-2'}}</b>
                              <div class="spancell">
                                ({{ get_compar_per(number_chenh_lech,total_passer_by_compare ) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </div>
                            </td>
                            <!-- tổng số lượng Kids Visits -->
                            <td [hidden]="indexOptionSelected !==indexes.kids_visitors ">
                              <b>{{total_kids  |  number:'1.0-2'}}</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.kids_visitors ">
                              <b>{{total_kids_compare > 0 ? ( total_kids_compare  |  number:'1.0-2') : 0}}</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.kids_visitors ">
                              <b>{{get_number_int(total_kids_compare,total_kids) | number:'1.0-2'}}</b>
                              <div class="spancell">
                                ({{ get_compar_per(number_chenh_lech,total_kids_compare ) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </div>
                            </td>
                            <!-- tổng số lượng Turn in rate -->
                            <td [hidden]="indexOptionSelected !==indexes.turn_in_rate ">
                              <b>{{total_turn_rate  |  number:'1.0-2'}}</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.turn_in_rate ">
                              <b>{{total_turn_rate_compare > 0 ? ( total_turn_rate_compare  |  number:'1.0-2') : 0}}</b>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.turn_in_rate ">
                              <b>{{get_number_int(total_turn_rate_compare,total_turn_rate) | number:'1.0-2'}}</b>
                              <div class="spancell">
                                ({{ get_compar_per(number_chenh_lech,total_turn_rate_compare ) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </div>
                            </td>
                            <!-- tổng số lượng Shopper Visits -->
                            <td [hidden]="indexOptionSelected !==indexes.shoppers ">
                              <b>{{total_shopper_visits  |  number:'1.0-2'}}</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.shoppers ">
                              <b>{{total_shopper_visits_compare > 0 ? ( total_shopper_visits_compare  |  number:'1.0-2') : 0}}</b>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.shoppers ">
                              <b>{{get_number_int(total_shopper_visits_compare,total_shopper_visits) | number:'1.0-2'}}</b>
                              <div class="spancell">
                                ({{ get_compar_per(number_chenh_lech,total_shopper_visits_compare ) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </div>
                            </td>

                            <!-- tổng số lượng traffic -->
                            <td [hidden]="indexOptionSelected !==indexes.traffic_flow">
                              <b>{{total_num_to_traffic |  number:'1.0-2'}}</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.traffic_flow">
                              <b>{{ total_num_to_traffic_compare > 0 ? (total_num_to_traffic_compare |  number:'1.0-2') : 0 }}</b>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.traffic_flow">
                              <b>{{get_number_int(total_num_to_traffic_compare,total_num_to_traffic) | number:'1.0-2'}}</b>
                              <div class="spancell">
                                ({{ get_compar_per(number_chenh_lech, total_num_to_traffic_compare) | number:'1.0-2' }}%)
                                <i class="fa fa-{{up_or_down}}">&nbsp;</i>
                              </div>
                            </td>
                            <!-- time trung bình  -->
                            <td [hidden]="indexOptionSelected !==indexes.avg_time ">
                              <b>{{toHHMMSS(total_avgtime)}}</b><br> <span class="spancell">Thời gian trung bình</span>
                            </td>
                            <td [hidden]="indexOptionSelected !==indexes.avg_time ">
                              <b>{{toHHMMSS(total_avgtime_compare)}}</b><br> <span class="spancell">Thời gian trung bình
                                được so sánh</span></td>
                            <td [hidden]="indexOptionSelected !==indexes.avg_time">
                              <b>{{toHHMMSS(get_number_int(total_avgtime,total_avgtime_compare))}}</b><br>
                              <span class="spancell">{{language.thoi_gian_chenh_lech}}</span>
                            </td>
                            <!-- tổng số lượng Conversion rate -->
                            <td [hidden]="indexOptionSelected !==indexes.conversion_rate">
                              <b>{{tt_conver |  number:'1.0-2'}} (%)</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.conversion_rate">
                              <b>{{ tt_conver_compare > 0 ? (tt_conver_compare |  number:'1.0-2') : 0 }} (%)</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.conversion_rate">
                              <b>{{get_number_int(tt_conver_compare,tt_conver) | number:'1.0-2'}}</b>
                              <div class="spancell">
