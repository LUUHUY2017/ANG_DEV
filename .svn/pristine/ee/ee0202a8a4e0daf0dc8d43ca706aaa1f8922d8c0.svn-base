                            <td [hidden]="indexOptionSelected !==indexes.sales_yield">
                              <b>{{tt_sales_yeild |  number:'1.0-2'}}</b></td>
                            <td [hidden]="indexOptionSelected !==indexes.sales_yield">
                              <b>{{ tt_sales_yeild_compare > 0 ? (tt_sales_yeild_compare |  number:'1.0-2') : 0 }}</b>
                            </td>
