:host ::ng-deep {
  .multiselect-dropdown {
    position: relative;
    width: 100%;
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.2), 0 1px 1px 0 rgba(0, 0, 0, 0.14), 0 2px 1px -1px rgba(0, 0, 0, 0.12) !important;
  }
  .multiselect-dropdown .dropdown-btn .dropdown-down {
    border-top: 7px solid #23282c !important;
    border-left: 4px solid transparent !important;
    border-right: 4px solid transparent !important;
  }
  .multiselect-dropdown .dropdown-btn .dropdown-up {
    border-bottom: 7px solid #23282c !important;
    border-left: 4px solid transparent !important;
    border-right: 4px solid transparent !important;
  }
  .multiselect-dropdown .dropdown-btn .selected-item {
    margin-right: 4px;
    padding: 0px 5px 2px 5px !important;
  }
  .multiselect-dropdown .dropdown-btn .selected-item:hover {
    box-shadow: unset !important;
  }
  .multiselect-dropdown .dropdown-btn {
    display: inline-block;
    border: 1px solid #dddddd !important;
    width: 100% !important;
    min-width: 130px;
    padding: 5px 2px 2px 2px !important;
    border-radius: 0px !important;
    margin-top: 0 !important;
  }
  .multiselect-dropdown .dropdown-btn .selected-item {
    border: 1px solid #ff9739 !important;
    background: #ff9739 !important;
  }
  /* Khung địa điểm*/
  .text-select {
    position: relative;
  }
  .khung-location-location {
    position: absolute;
    left: 2%;
    width: 98%;
    height: 250px;
    overflow: hidden;
    background: white;
    border-radius: 3px;
    z-index: 10;
    box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14),
      0px 3px 14px 2px rgba(0, 0, 0, 0.12) !important;
  }
  .khung-location-location #khung-location-scroll {
    overflow-y: scroll;
    height: 100%;
  }
  ul#ul-location {
    padding: 0;
    margin: 0;
  }
  .khung-location-location li {
    padding: 8px 0 8px 8px;
    cursor: pointer;
    padding-left: 10px;
    list-style: none;
  }
  .khung-location-location > input {
    padding: 20px 8px 8px 8px;
  }
  .khung-location-location input:focus {
    color: #5c6873;
    background-color: #fff;
    border-color: none;
    outline: 0;
    box-shadow: 0 0 0 0.2rem rgba(255, 255, 255, 0.25);
  }
  .khung-location-location > li:hover {
    background-color: rgba(0, 0, 0, 0.08);
  }
  .location {
    font-size: 17px;
  }
  .khung-location-location .menu-0 {
    width: 100%;
    font-weight: bold;
  }
  .khung-location-location .menu-1 {
    margin-left: 25px;
    border-left: 1px dotted #a19292;
    width: calc(100% - 25px) !important;
  }
  .khung-location-location .menu-2 {
    margin-left: 35px;
    // border-left: 1px dotted #a19292;
    width: calc(100% - 35px) !important;
  }
  .khung-location-location .menu-3 {
    margin-left: 45px;
    border-left: 1px dotted #a19292;
    width: calc(100% - 45px) !important;
  }
  .khung-location-location .menu-4 {
    margin-left: 55px;
    border-left: 1px dotted #a19292;
    width: calc(100% - 55px) !important;
  }
  .khung-location-location .menu-5 {
    margin-left: 65px;
    border-left: 1px dotted #a19292;
    width: calc(100% - 65px) !important;
  }
  .khung-location-location .menu-6 {
    margin-left: 75px;
    border-left: 1px dotted #a19292;
    width: calc(100% - 75px) !important;
  }
  li.active {
    color: #1f8ad9 !important;
  }
  .dropdown-list li {
    padding: 1px 3px !important;
  }
  .showform {
    visibility: visible !important;
    opacity: 1 !important;
    display: block !important;
  }
  .card-form {
    display: none;
    visibility: hidden;
    opacity: 0;
  }
  /* list- menu*/
  .title-step {
    cursor: pointer;
  }
  .title-step.active {
    background: #ff9739;
    color: white;
  }

  .form-control {
    border-radius: unset !important;
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.15), 0 0 0px rgba(82, 168, 236, 0.2);
  }
}

