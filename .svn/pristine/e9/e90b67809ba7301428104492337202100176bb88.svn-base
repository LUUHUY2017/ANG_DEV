.manual-page-tab {
  margin-bottom: 2.5rem;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-flow: row nowrap;
  flex-flow: row nowrap;
  border-radius: 5px;
  overflow: hidden;
  background: #eee;
}

.nav {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  width: 100% !important;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  padding-left: 0;
  margin-bottom: 0;
  list-style: none;
}
.manual-page-tab .nav-link.active {
  color: #ff9739;
  background: #fff;
}

.manual-page-tab .nav-link {
  color: #ff9739;
  background: #fff;
}
.manual-page-tab .nav-link.active {
  color: #fff;
  background: #ff9739;
  font-weight: 500;
}
.manual-page-tab .nav-link {
  line-height: 1.3;
  padding: 0 5px 0 20px;
  height: 45px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-flow: row nowrap;
  flex-flow: row nowrap;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  text-align: center;
  font-size: 1.2rem;
  color: #212121;
  position: relative;
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
  position: relative;
  background: #e0e0e0;
  font-weight: 500;
}
.manual-page-tab .nav-item {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
}
.nav-link {
  display: block;
  padding: 0.5rem 1rem;
}

.manual-page-tab .nav-link.active::before {
  border-left: 16px solid #fff;
}

.manual-page-tab .nav-link:before {
  border-left: 16px solid #fff;
}
.manual-page-tab .nav-link.active::before {
  border-left: 16px solid#ff9739;
}
.manual-page-tab .nav-link:before {
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
  content: " ";
  display: block;
  width: 0;
  height: 0;
  border-top: 26px solid transparent;
  border-bottom: 26px solid transparent;
  border-left: 16px solid #e0e0e0;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  left: 100%;
  z-index: 3;
  margin-left: -2px;
}

.manual-page-tab .nav-link:after {
  border-top: 30px solid transparent;
  border-bottom: 30px solid transparent;
  border-left: 18px solid#ff9739;
}

.manual-page-tab .nav-link:after {
  margin-left: -1px;
  content: " ";
  display: block;
  width: 0;
  height: 0;
  border-top: 30px solid transparent;
  border-bottom: 30px solid transparent;
  border-left: 18px solid #fff;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  left: 100%;
  z-index: 2;
}
