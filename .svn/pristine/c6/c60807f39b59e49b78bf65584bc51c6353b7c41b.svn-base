// giám sát thiết bị
span.online {
  font-size: 11px;
  animation-duration: 3s;
  animation-name: online;
  animation-iteration-count: infinite;
  animation-direction: alternate;
  i {
    font-size: 11px;
    animation-duration: 3s;
    animation-name: online;
    animation-iteration-count: infinite;
    animation-direction: alternate;
  }
}
@keyframes online {
  from {
    color: #79a789;
  }
  to {
    // color: #229e4d;
    // color: #0cf15b;
    color: green;
  }
}
span.offline {
  font-size: 11px;
  animation-duration: 3s;
  animation-name: offline;
  animation-iteration-count: infinite;
  animation-direction: alternate;
  i {
    font-size: 11px;
    animation-duration: 3s;
    animation-name: offline;
    animation-iteration-count: infinite;
    animation-direction: alternate;
  }
}
@keyframes offline {
  from {
    color: salmon;
  }
  to {
    // color: #229e4d;
    color:red;
  }
}

/* Khung địa điểm*/
.khung-location-location{
	position: absolute; 
  left: 4%;
  width: 92.2%;
	height: 220px;
	overflow: hidden ;
	background: white;
	border-radius: 3px;
	z-index: 10;
	box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14), 0px 3px 14px 2px rgba(0, 0, 0, 0.12) !important;
}
.khung-location-location #khung-location-scroll{
	overflow-y: scroll; 
	height: 100%;
}
ul#ul-location{
	padding: 0;
	margin: 0;
}
.khung-location-location li{
	padding: 8px 0 8px 8px; ;
	cursor: pointer;
	padding-left: 10px ;
	list-style: none;
}
.khung-location-location > input{
	padding: 20px 8px 8px 8px;
}
.khung-location-location input:focus{
    color: #5c6873;
    background-color: #fff;
    border-color: none;
    outline: 0;
    box-shadow: 0 0 0 0.2rem rgba(255, 255, 255, 0.25);
}
.khung-location-location > li:hover{
	background-color: rgba(0, 0, 0, 0.08);
}
.location{
	font-size: 17px;
}
.khung-location-location .menu-0
{
	width:100%;
}
.khung-location-location .menu-5
{
	margin-left: 25px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 25px)!important;
}
.khung-location-location .menu-10
{
	margin-left: 35px;
    border-left: 1px dotted #a19292;
	width: calc(100% - 35px)!important;
}
.khung-location-location .menu-15
{
	margin-left: 45px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 45px)!important;
}
.khung-location-location .menu-20
{
	margin-left: 55px;
    border-left: 1px dotted #a19292;
	width: calc(100% - 55px)!important;
}
.khung-location-location .menu-25
{
	margin-left: 65px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 65px)!important;
}
.khung-location-location .menu-30
{
	margin-left: 75px;
	border-left: 1px dotted #a19292;
	width: calc(100% - 75px)!important;
}
li.active
{
    color: #1f8ad9!important
}

.tablets-list_filter_table 
{
  margin-left: 0px;
  margin-right: 0px;
}
.tablets-list_filter_table ul
{
    width: 100%;
	  padding: 0;
    height: 30px;
    margin-bottom: 0px;
}
.tablets-list_filter_table li
{
    list-style: none;
}
.tablets-list_filter_table li>a
{
    color: #20a8d8;
    cursor: pointer;
}
.tablets-list_filter_table li>a:hover
{
    text-decoration: underline;
}
.tablets-list_filter_table li:nth-child(1)
{
    float: left;
}
.tablets-list_filter_table li:nth-child(2)
{
    float: right;
}
.tablets-list_filter_table li:nth-child(2) .input_filter_table
{
	width: 130px;
	padding-left: 5px;
	box-shadow: inset 0 1px 1px rgba(0,0,0,0.15), 0 0 0px rgba(82,168,236,.2);   
	outline: none;
	border: 1px solid #ddd;
}
.tablets-list_filter_table li:nth-child(2) .input_filter_table:focus,.tablets-list_filter_table .select_cus_table:focus
{
	/* box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);    */
    border-color: rgba(82,168,236,.8);
}
.tablets-list_filter_table .select_cus_table
{
	box-shadow: inset 0 1px 1px rgba(0,0,0,0.15), 0 0 0px rgba(82,168,236,.2);   
}
