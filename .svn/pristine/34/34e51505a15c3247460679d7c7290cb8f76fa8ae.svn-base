} */

.table-companies table thead th {
  border-bottom: 1px solid #c8ced3 !important;
  border-top: unset !important;
}

.table-companies .table td {
  vertical-align: middle;
}

.modal-footer, .modal-header {
  padding: 5px 1rem;
}

tfoot.companies_table-tfoot ul.pagination {
  float: right;
  margin-bottom: -1rem;
}

tfoot.companies_table-tfoot tr td {
  padding: 3px 0 3px 3px;
}

.actived_record {
  border: 2px solid goldenrod;
}

.iconAddPlus {
  color: #4dbd74;
  font-size: 24px;
  cursor: pointer;
  padding: 3px 10px;
  border: 1px solid;
  border-radius: 5px;
  transition: 0.3s;
}
.iconAddPlus:hover {
  background: #4dbd74;
  color: #fff;
}
.iconRemovePlus {
  border-radius: 5px;
  color: crimson;
  cursor: pointer;
  border: 1px solid;
  padding: 0px 10px;
  transition: 0.3s;
}
.iconRemovePlus:hover {
  background: crimson;
  color: #fff;
}
