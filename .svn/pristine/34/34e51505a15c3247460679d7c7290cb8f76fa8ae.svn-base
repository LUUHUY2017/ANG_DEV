/* Khung địa điểm*/

.khung-location-administrator {
  position: absolute;
  left: 4.2%;
  width: 92%;
  height: 220px;
  overflow: hidden;
  background: white;
  border-radius: 3px;
  z-index: 10;
  box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14), 0px 3px 14px 2px rgba(0, 0, 0, 0.12) !important;
}

.khung-location-administrator #khung-location-scroll {
  overflow-y: scroll;
  height: 100%;
}

ul#ul-location {
  padding: 0;
  margin: 0;
}

.khung-location-administrator li {
  padding: 8px 0 8px 8px;
  ;
  cursor: pointer;
  padding-left: 10px;
  list-style: none;
}

.khung-location-administrator>input {
  padding: 20px 8px 8px 8px;
}

.khung-location-administrator input:focus {
  color: #5c6873;
  background-color: #fff;
  border-color: none;
  outline: 0;
  box-shadow: 0 0 0 0.2rem rgba(255, 255, 255, 0.25);
}

.khung-location-administrator>li:hover {
  background-color: rgba(0, 0, 0, 0.08);
}

.location {
  font-size: 17px;
}

.khung-location-administrator .menu-0 {
  width: 100%;
  font-weight: bold;
}

.khung-location-administrator .menu-1 {
  margin-left: 25px;
  border-left: 1px dotted #a19292;
  width: calc(100% - 25px)!important;
}

.khung-location-administrator .menu-2 {
  margin-left: 35px;
  border-left: 1px dotted #a19292;
  width: calc(100% - 35px)!important;
}

.khung-location-administrator .menu-3 {
  margin-left: 45px;
  border-left: 1px dotted #a19292;
  width: calc(100% - 45px)!important;
}

.khung-location-administrator .menu-4 {
  margin-left: 55px;
  border-left: 1px dotted #a19292;
  width: calc(100% - 55px)!important;
}

.khung-location-administrator .menu-5 {
  margin-left: 65px;
  border-left: 1px dotted #a19292;
  width: calc(100% - 65px)!important;
}

.khung-location-administrator .menu-6 {
  margin-left: 75px;
  border-left: 1px dotted #a19292;
  width: calc(100% - 75px)!important;
}

li.active {
  color: #1f8ad9!important
}
.disabled {
  pointer-events: none;
  opacity: 0.6;
}
.custom-select-ad {
  transition: 0.1s;
}
.custom-select-ad {
  box-shadow:0 1px 3px 0 rgba(0, 0, 0, 0.2), 0 1px 1px 0 rgba(0, 0, 0, 0.14), 0 2px 1px -1px rgba(0, 0, 0, 0.12) !important;
}

.trhover:hover > td {
  font-family: Open-semi;
}